  "private": true,
  "dependencies": {
    "@babel/runtime": "^7.14.6",
    "classnames": "^2.3.1",
    "core-js": "^3.15.2",
    "faker": "^5.5.3",
    "fast-check": "^2.17.0",
    "graphql": "^14.5.8",
    "meteor-node-stubs": "^1.0.3",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^1.3.1",
    "simpl-schema": "^1.12.0",
    "sweetalert": "^2.1.2",
    "uniforms": "^3.5.1",
    "uniforms-bridge-simple-schema-2": "^3.5.1",
    "uniforms-semantic": "^3.5.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.30.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-meteor": "^7.3.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-utils": "^2.1.0",
    "puppeteer": "^10.1.0",
    "testcafe": "^1.14.2"
  },
  "scripts": {
    "start": "meteor --no-release-check --exclude-archs web.browser.legacy,web.cordova --settings ../config/settings.development.json",
    "lint": "eslint --quiet --fix --ext .jsx --ext .js ./imports",
    "test-unit": "cross-env TEST_BROWSER_DRIVER=puppeteer MOCHA_TIMEOUT=150000 meteor test --exclude-archs web.browser.legacy,web.cordova --no-release-check --once --driver-package meteortesting:mocha --port 3100",
    "testcafe-development": "testcafe chrome tests/*.testcafe.js",
    "testcafe": "testcafe chrome:headless tests/*.testcafe.js -q --app \"meteor npm run start\""
  }
}
